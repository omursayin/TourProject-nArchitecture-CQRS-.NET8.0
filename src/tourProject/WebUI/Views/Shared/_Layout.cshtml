@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HeadViewComponentPartial")

<body>

    @await Component.InvokeAsync("_NavbarViewComponentPartial")

    @await Component.InvokeAsync("_NavbarCoverImageViewComponentPartial")

    @RenderBody()

    @await Component.InvokeAsync("_FooterViewComponentPartial")

    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

    @await Component.InvokeAsync("_ScriptsViewComponentPartial")

</body>
</html>
